<!-- 		from SourceOfSample sos order by sos.domain, sos.description -->
<!-- 	</query> -->
<!-- 	<query name="sourceofsample.getPrevious"> -->
<!-- 		from SourceOfSample sos order by sos.domain desc, sos.description desc -->
<!-- 	</query> -->
</hibernate-mapping>
