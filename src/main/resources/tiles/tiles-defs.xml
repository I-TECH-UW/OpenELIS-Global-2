		template="/pages/menu/masterListsTab/masterListsPage.jsp">
		<put-attribute name="left"
			value="/pages/menu/masterListsTab/masterListsPageLeft.jsp" />
		<put-attribute name="right"
			value="masterListsPageRightDefinition" />
	</definition>

